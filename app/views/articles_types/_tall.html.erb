<div class="col-sm-3 post post-10">
  <div class="row m0 inner">
    <div class="row m0 featured_img">
      <%= link_to article_path(article) do %>
          <%= image_tag article.image.url(:large) ,class:'img-responsive'%>
      <%end%>
    </div>
    <div class="row m0 post_contents">
      <div class="row m0 category money">
        <a href="#">money</a>
      </div>
      <h3 class="post_title"><%= link_to article.title , article_path(article)%></h3>
      <ul class="post_meta nav nav-pills">
        <li><%=article.created_at.strftime("%A, %b %d")%></li>
        <li style="color:white;">By <%=article.user.name%></li>
      </ul>
    </div>
  </div>
</div><!--Post-->
