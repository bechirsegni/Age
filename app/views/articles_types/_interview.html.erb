<div class="col-sm-6 post interview">
  <div class="row m0 inner">
    <%= image_tag article.image.url(:med_home) ,class:'img-responsive'%>
    <div class="dropdown pull-right social_share_drop">
      <button type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        <i class="fa fa-ellipsis-v"></i>
      </button>
      <ul class="dropdown-menu">
        <li><a href="#"><i class="fa fa-facebook"></i> Facebook</a></li>
        <li><a href="#"><i class="fa fa-twitter"></i> Twitter</a></li>
        <li><a href="#"><i class="fa fa-google-plus"></i> Google Plus</a></li>
        <li><a href="#"><i class="fa fa-pinterest"></i> Pinterest</a></li>
        <li><a href="#"><i class="fa fa-linkedin"></i> LinkedIn</a></li>
      </ul>
    </div>
    <div class="row m0 post_contents">
      <h4 class="category <%=article.category.name%> "><a href="#"><%=article.category.name%></a></h4>
      <h2 class="title"><%=link_to article.title, article_path(article), style: 'color:white'%></h2>
      <ul class="post_meta nav nav-pills">
        <li><a href="#"><%=article.created_at.strftime("%A, %b %d")%></a></li>
        <li><a href="#"><i class="fa fa-thumbs-o-up"></i>1456</a></li>
        <li><a href="#"><i class="fa fa-commenting"></i>32</a></li>
      </ul>
    </div>
  </div>
</div>